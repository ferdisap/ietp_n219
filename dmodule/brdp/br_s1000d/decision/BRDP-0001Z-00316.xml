<brDecision
  brDecisionIdentNumber="BRDP-S1-00316"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../schema/flat/brdoc.xsd"
