      <contextRules>
        <structureObjectRuleGroup>
          <structureObjectRule id="sbr-001">
            <brDecisionRef brDecisionIdentNumber="0001Z-00001"/>
            <objectPath allowedObjectFlag="0">//dmCode/@identCode | //dmCode/@infoCodeVariant | //issueInfo/@inwork </objectPath>
            <objectUse>Those of attribute are not allowed to have letter I and O inside the value.</objectUse>
            <objectValue valueForm="pattern" valueAllowed="[iIoO]">the matched pattern is not allowed</objectValue>
          </structureObjectRule>
          <structureObjectRule id="sbr-002">
            <brDecisionRef brDecisionIdentNumber="0001Z-00002"/>
            <brDecisionRef brDecisionIdentNumber="0001Z-00070"/>
            <brDecisionRef brDecisionIdentNumber="0001Z-00071"/>
            <objectPath allowedObjectFlag="1">//originator/@enterpriseCode='0001Z'</objectPath>
            <objectUse>The XPath result shall be true.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-003">
            <brDecisionRef brDecisionIdentNumber="0001Z-00014"/>
            <brDecisionRef brDecisionIdentNumber="0001Z-00054"/>
            <brDecisionRef brDecisionIdentNumber="0001Z-00558"/>
            <objectPath allowedObjectFlag="1">not(//@caveat) and not(//commercialClassification) and not(//derivativeClassification)</objectPath>
            <objectUse>The XPath result should be true. The attribute caveats must not be used in data module.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-004">
            <brDecisionRef brDecisionIdentNumber="0001Z-00020"/>
            <objectPath allowedObjectFlag="1">//identAndStatusSection/dmAddress/dmIdent/languange/@countryIsoCode = 'GB' and //identAndStatusSection/dmAddress/dmIdent/languange/@languangeIsoCode = 'en'</objectPath>
            <objectUse>The XPath result shall be true.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-005">
            <brDecisionRef brDecisionIdentNumber="0001Z-00095"/>
            <objectPath allowedObjectFlag="1">not(//title/internalRef) and not(//title/dmRef) and not(//title/pmRef) </objectPath>
            <objectUse>The XPath result shall be true. Element &#60;internalRef&#62; and &#60;dmRef&#62; and &#60;pmRef&#62; are not allowed.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-006">
            <brDecisionRef brDecisionIdentNumber="0001Z-00032"/>
            <brDecisionRef brDecisionIdentNumber="0001Z-00033"/>
            <objectPath allowedObjectFlag="1">not(//warning/@vitalWarningFlag) | not(//warning/@warningType)</objectPath>
            <objectUse>The XPath result shall be true. attribute vitalWarningFlag and  warningType is not allowed.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-007">
            <brDecisionRef brDecisionIdentNumber="0001Z-00034"/>            
            <objectPath allowedObjectFlag="1">not(//caution/@cautionType)</objectPath>
            <objectUse>The XPath result shall be true. attribute cautionType is not allowed.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-008">
            <brDecisionRef brDecisionIdentNumber="0001Z-00035"/>            
            <objectPath allowedObjectFlag="1">not(//note/@noteType)</objectPath>
            <objectUse>The XPath result shall be true. attribute noteType is not allowed.</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-009">
            <brDecisionRef brDecisionIdentNumber="0001Z-00090"/>            
            <objectPath allowedObjectFlag="0">//issueInfo[@inWork != '00']/ancestor::dmodule//reasonForUpdate</objectPath>
            <objectUse>The XPath result shall be 'not match'. Element &#60;reasonForUpdate&#62; is not allowed during production proccess (@inwork != 00).</objectUse>
          </structureObjectRule>
          <structureObjectRule id="sbr-010">
            <brDecisionRef brDecisionIdentNumber="0001Z-00118"/>            
            <objectPath allowedObjectFlag="0">//title/parent::randomList | //title/parent::sequentialList | //title/parent::attentionSequentialList</objectPath>
