<content 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="crew.xsd"
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://www.purl.org/dc/elements/1.1/">

    <crew>
      <descrCrew>
        <levelledPara>
          <title>On Ground Emergencies</title>

          <levelledPara>
            <title>Engine Fire On Groundf</title>
            <crewDrill orderedStepsFlag="1">
                <title>Action</title>
                <crewDrillStep memorizeStepsFlag="0" separatorStyle="dot" orderedStepsFlag="1">
                  <challengeAndResponse>
                    <challenge><para>POWER levers (both engines)</para></challenge>
                    <crewMemberGroup>
                      <crewMember crewMemberType="cm02"/>
                      <crewMember crewMemberType="cm03"/>
                    </crewMemberGroup>
                    <response><para>IDLE</para></response>
                  </challengeAndResponse>
                </crewDrillStep>
                <crewDrillStep memorizeStepsFlag="1" separatorStyle="line" orderedStepsFlag="0">
                  <challengeAndResponse>
                    <challenge><para>PARKING brake</para></challenge>
                    <crewMemberGroup>
                      <crewMember crewMemberType="cm03"/>
                    </crewMemberGroup>
                    <response><para>Set</para></response>
                  </challengeAndResponse>
                  <crewDrillStep>
                    <challengeAndResponse>
                      <challenge><para>PARKING brake</para></challenge>
                      <response><para>Set</para></response>
                    </challengeAndResponse>
                  </crewDrillStep>
                  <crewDrillStep>
                    <challengeAndResponse>
                      <challenge><para>PEDAL brake</para></challenge>
                      <crewMemberGroup>
                        <crewMember crewMemberType="cm03"/>
                      </crewMemberGroup>
                      <response><para>Push</para></response>
                    </challengeAndResponse>
                  </crewDrillStep>
                </crewDrillStep>
            </crewDrill>
          </levelledPara>

        </levelledPara>
      </descrCrew>
    </crew>

