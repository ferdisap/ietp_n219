<captionGroup>
  <captionBody>
    <captionRow>
      <captionEntry alignCaptionEntry="right" valign="middle">
        <caption color="co83" captionWidth="30mm" captionHeight="10mm">
          <captionLine>FIRE - ENGINE</captionLine>
        </caption>
      </captionEntry>
      <captionEntry alignCaptionEntry="center" valign="middle">
        <caption color="co83" captionWidth="30mm" captionHeight="40mm">
          <captionLine>LOSS - ENGINE</captionLine>
        </caption>
      </captionEntry>
    </captionRow>
    <captionRow>
      <captionEntry alignCaptionEntry="center" valign="middle">
        <caption color="co83" captionWidth="70mm" captionHeight="20mm">
          <captionLine>FIRE - ENGINE</captionLine>
        </caption>
      </captionEntry>
      <captionEntry alignCaptionEntry="center" valign="bottom">
        <caption color="co83" captionWidth="50mm">
          <captionLine>LOSS - ENGINE</captionLine>
        </caption>
      </captionEntry>
    </captionRow>
  </captionBody>
</captionGroup>


